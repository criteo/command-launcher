pkgName: system-pkg-demo
version: 0.0.1
cmds:
  - name: __login__
    type: system
    executable: "{{.PackageDir}}/login.{{if eq .Os \"windows\"}}bat{{else}}sh{{end}}"
  - name: __metrics__
    type: system
    executable: "{{.PackageDir}}/metrics.{{if eq .Os \"windows\"}}bat{{else}}sh{{end}}"
  - name: __setup__
    type: system
    executable: "{{.PackageDir}}/setup.{{if eq .Os \"windows\"}}bat{{else}}sh{{end}}"
